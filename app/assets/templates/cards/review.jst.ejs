<ul class="card-attrs cube-wrap">
  <li class="cube">
    <div class="front-face">
      <strong>
      <% if (card.escape('image')) { %>
          <img src="<%= card.escape('image') %>" alt="" />
      <% } else { %>
        <%= card.escape("front") %>
      <% } %>
      </strong>
    </div>
    <div class="bottom-face">
      <strong>
        <% if (card.escape('format') === "field") { %>
          <% for (attr in card.get('back')) { %>
            <%= card.get('back')[attr] %>
          <% } %>
        <% } else { %>
          <%= card.escape("back") %>
        <% } %>
      </strong>
    </div>
  </li>
</ul>
